{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*).(ts|tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:ts|tsx|js|css|json|png|svg)).*)",
      "destination": "/index.html"
    }
  ]
}
